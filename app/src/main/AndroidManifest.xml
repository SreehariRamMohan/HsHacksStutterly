<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hshacksstutterly.hshacksstutterly">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity
            android:name=".HomeScreen"
            android:label="@string/title_activity_home_screen" />
        <activity android:name=".SpeechActivity" />
        <activity android:name=".Goals" />
        <activity android:name=".Summary" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".AnalyzePerformance" />
        <activity android:name=".PieChart" />
        <activity android:name=".CalendarActivity" />
        <activity
            android:name=".NavigationMain"
            android:label="@string/title_activity_navigation_main"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>